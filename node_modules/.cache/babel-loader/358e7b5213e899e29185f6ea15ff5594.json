{"ast":null,"code":"import \"../Firebase\";\nimport { getAuth, signInWithEmailAndPassword } from \"firebase/auth\";\nconst auth = getAuth();\nimport router from \"../router\";\nexport default {\n  name: \"LoginView\",\n\n  data() {\n    return {\n      user: {\n        email: \"\",\n        password: \"\"\n      },\n      messageError: \"\"\n    };\n  },\n\n  methods: {\n    loginUser() {\n      signInWithEmailAndPassword(auth, this.user.email, this.user.password).then(dataUser => {\n        localStorage.setItem(\"uidUser\", dataUser.user.uid);\n        router.push(\"/products\");\n      }).catch(() => {\n        this.messageError = \"invalid email and password\";\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwCA,OAAO,aAAP;AACA,SAASA,OAAT,EAAkBC,0BAAlB,QAAoD,eAApD;AACA,MAAMC,IAAG,GAAIF,OAAO,EAApB;AACA,OAAOG,MAAP,MAAmB,WAAnB;AACA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE,EADH;QAEJC,QAAQ,EAAE;MAFN,CADD;MAKLC,YAAY,EAAE;IALT,CAAP;EAOD,CAVY;;EAWbC,OAAO,EAAE;IACPC,SAAS,GAAG;MACVV,0BAA0B,CAACC,IAAD,EAAO,KAAKI,IAAL,CAAUC,KAAjB,EAAwB,KAAKD,IAAL,CAAUE,QAAlC,CAA1B,CACGI,IADH,CACSC,QAAD,IAAc;QAClBC,YAAY,CAACC,OAAb,CAAqB,SAArB,EAAgCF,QAAQ,CAACP,IAAT,CAAcU,GAA9C;QACAb,MAAM,CAACc,IAAP,CAAY,WAAZ;MACD,CAJH,EAKGC,KALH,CAKS,MAAM;QACX,KAAKT,YAAL,GAAoB,4BAApB;MACD,CAPH;IAQD;;EAVM;AAXI,CAAf","names":["getAuth","signInWithEmailAndPassword","auth","router","name","data","user","email","password","messageError","methods","loginUser","then","dataUser","localStorage","setItem","uid","push","catch"],"sourceRoot":"","sources":["/home/rani/Desktop/programmation/web dev/myprojectvuejs/src/views/LoginView.vue"],"sourcesContent":["<template>\n  <div class=\"col-md-6 col-lg-4 offset-lg-4 offset-md-3 mt-5\">\n    <div class=\"bg-light p-5 border shadow\">\n      <!-- Login Form -->\n      <form>\n        <div class=\"mb-4\">\n          <input\n            type=\"email\"\n            class=\"form-control\"\n            placeholder=\"Username/Email\"\n          />\n          <p class=\"form-text text-end\">Enter Valid Username/Email</p>\n        </div>\n        <div class=\"mb-4\">\n          <input\n            type=\"password\"\n            class=\"form-control\"\n            placeholder=\"Enter Password\"\n          />\n          <p class=\"form-text text-end\">Enter Valid Password</p>\n        </div>\n        <div class=\"mb-4 form-check w-100\">\n          <label class=\"form-check-label\">\n            <input type=\"checkbox\" class=\"form-check-input\" /> Remember Me\n          </label>\n          <a href=\"#\" class=\"float-end\">Reset Password</a>\n        </div>\n        <button type=\"submit\" class=\"btn btn-primary w-100 my-3 shadow\">\n          Login\n        </button>\n        <p class=\"text-center m-0\">\n          Not yet account, <a href=\"#\">Please Signup</a>\n        </p>\n      </form>\n      <!-- Login Form -->\n    </div>\n  </div>\n</template>\n\n<script>\nimport \"../Firebase\";\nimport { getAuth, signInWithEmailAndPassword } from \"firebase/auth\";\nconst auth = getAuth();\nimport router from \"../router\";\nexport default {\n  name: \"LoginView\",\n  data() {\n    return {\n      user: {\n        email: \"\",\n        password: \"\",\n      },\n      messageError: \"\",\n    };\n  },\n  methods: {\n    loginUser() {\n      signInWithEmailAndPassword(auth, this.user.email, this.user.password)\n        .then((dataUser) => {\n          localStorage.setItem(\"uidUser\", dataUser.user.uid);\n          router.push(\"/products\");\n        })\n        .catch(() => {\n          this.messageError = \"invalid email and password\";\n        });\n    },\n  },\n};\n</script>"]},"metadata":{},"sourceType":"module"}