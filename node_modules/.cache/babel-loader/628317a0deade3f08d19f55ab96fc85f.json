{"ast":null,"code":"// @ is an alias to /src\nimport \"../Firebase\";\nimport { getAuth, createUserWithEmailAndPassword } from \"firebase/auth\";\nimport { getFirestore, collection, setDoc } from \"firebase/firestore\";\nconst db = getFirestore(app);\nconst auth = getAuth();\nconst ref = collection(db, \"users\");\nimport router from \"../router\";\nexport default {\n  name: \"SignUpView\",\n\n  data() {\n    return {\n      user: {\n        name: \"\",\n        email: \"\",\n        password: \"\",\n        uid: \"\"\n      }\n    };\n  },\n\n  methods: {\n    signupUser() {\n      createUserWithEmailAndPassword(auth, this.user.email, this.user.password).then(dataUser => {\n        // this.ref.add({\n        //   name: this.user.name,\n        //   email: this.user.email,\n        //   uid: dataUser.user.uid,\n        // });\n        // Add a new document in collection \"cities\"\n        this.ref.setDoc({\n          name: this.user.name,\n          email: this.user.email,\n          uid: dataUser.user.uid\n        });\n        router.push(\"/products\");\n      }).catch(error => {\n        console.log(error); // ..\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2CA;AACA,OAAO,aAAP;AACA,SAASA,OAAT,EAAkBC,8BAAlB,QAAwD,eAAxD;AACA,SAASC,YAAT,EAAuBC,UAAvB,EAAmCC,MAAnC,QAAiD,oBAAjD;AACA,MAAMC,EAAC,GAAIH,YAAY,CAACI,GAAD,CAAvB;AACA,MAAMC,IAAG,GAAIP,OAAO,EAApB;AACA,MAAMQ,GAAE,GAAIL,UAAU,CAACE,EAAD,EAAK,OAAL,CAAtB;AACA,OAAOI,MAAP,MAAmB,WAAnB;AACA,eAAe;EACbC,IAAI,EAAE,YADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJF,IAAI,EAAE,EADF;QAEJG,KAAK,EAAE,EAFH;QAGJC,QAAQ,EAAE,EAHN;QAIJC,GAAG,EAAE;MAJD;IADD,CAAP;EAQD,CAXY;;EAYbC,OAAO,EAAE;IACPC,UAAU,GAAG;MACXhB,8BAA8B,CAACM,IAAD,EAAO,KAAKK,IAAL,CAAUC,KAAjB,EAAwB,KAAKD,IAAL,CAAUE,QAAlC,CAA9B,CACGI,IADH,CACSC,QAAD,IAAc;QAClB;QACA;QACA;QACA;QACA;QACA;QACA,KAAKX,GAAL,CAASJ,MAAT,CAAgB;UACdM,IAAI,EAAE,KAAKE,IAAL,CAAUF,IADF;UAEdG,KAAK,EAAE,KAAKD,IAAL,CAAUC,KAFH;UAGdE,GAAG,EAAEI,QAAQ,CAACP,IAAT,CAAcG;QAHL,CAAhB;QAMAN,MAAM,