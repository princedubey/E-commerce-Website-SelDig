{"ast":null,"code":"// @ is an alias to /src\nimport \"../Firebase\";\nimport { addDoc, collection, query, getFirestore, onSnapshot } from \"firebase/firestore\";\nconst db = getFirestore();\nconst q = query(collection(db, \"products\"));\nexport default {\n  name: \"MyProductsView\",\n\n  data() {\n    return {\n      product: {\n        name: \"\",\n        price: 0,\n        image: \"\",\n        description: \"\",\n        uid: localStorage.getItem(\"uidUser\")\n      },\n      messageSuccess: \"\",\n      products: []\n    };\n  },\n\n  methods: {\n    AddProduct() {\n      addDoc(q, this.product).then(() => {\n        this.messageSuccess = \"Added\";\n        this.product.name = \"\";\n        this.product.price = \"\";\n        this.product.image = \"\";\n        this.product.description = \"\";\n      });\n    }\n\n  },\n\n  created() {\n    onSnapshot(q, querySnapshot => {\n      this.products = [];\n      querySnapshot.forEach(doc => {\n        if (doc.data().uid == this.product.uid) {\n          this.products.push({\n            key: doc.id,\n            name: doc.data().name,\n            description: doc.data().description,\n            price: doc.data().price,\n            image: doc.data().image\n          });\n        }\n      });\n    });\n  }\n\n};","map":{"version":3,"mappings":"AA8DA;AACA,OAAO,aAAP;AAEA,SACEA,MADF,EAEEC,UAFF,EAGEC,KAHF,EAIEC,YAJF,EAKEC,UALF,QAMO,oBANP;AAOA,MAAMC,EAAC,GAAIF,YAAY,EAAvB;AACA,MAAMG,IAAIJ,KAAK,CAACD,UAAU,CAACI,EAAD,EAAK,UAAL,CAAX,CAAf;AACA,eAAe;EACbE,IAAI,EAAE,gBADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPF,IAAI,EAAE,EADC;QAEPG,KAAK,EAAE,CAFA;QAGPC,KAAK,EAAE,EAHA;QAIPC,WAAW,EAAE,EAJN;QAKPC,GAAG,EAAEC,YAAY,CAACC,OAAb,CAAqB,SAArB;MALE,CADJ;MAQLC,cAAc,EAAE,EARX;MASLC,QAAQ,EAAE;IATL,CAAP;EAWD,CAdY;;EAebC,OAAO,EAAE;IACPC,UAAU,GAAG;MACXnB,MAAM,CACJM,CADI,EAGJ,KAAKG,OAHD,CAAN,CAIEW,IAJF,CAIO,MAAM;QACX,KAAKJ,cAAL,GAAsB,OAAtB;QACA,KAAKP,OAAL,CAAaF,IAAb,GAAoB,EAApB;QACA,KAAKE,OAAL,CAAaC,KAAb,GAAqB,EAArB;QACA,KAAKD,OAAL,C