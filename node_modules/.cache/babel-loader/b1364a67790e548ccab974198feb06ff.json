{"ast":null,"code":"import \"./Firebase\";\nimport { getAuth, signOut, onAuthStateChanged } from \"firebase/auth\";\nimport router from \"./router\";\nconst auth = getAuth();\nexport default {\n  name: \"App\",\n\n  data() {\n    return {\n      CheckAuth: null\n    };\n  },\n\n  methods: {\n    logout() {\n      signOut(auth).then(() => {\n        router.push(\"/\");\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  },\n\n  created() {\n    onAuthStateChanged(auth, user => {\n      if (user) {\n        this.CheckAuth = user;\n      } else {\n        this.CheckAuth = null;\n      }\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAoDA,OAAO,YAAP;AACA,SAASA,OAAT,EAAkBC,OAAlB,EAA2BC,kBAA3B,QAAqD,eAArD;AACA,OAAOC,MAAP,MAAmB,UAAnB;AACA,MAAMC,IAAG,GAAIJ,OAAO,EAApB;AACA,eAAe;EACbK,IAAI,EAAE,KADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MAAEC,SAAS,EAAE;IAAb,CAAP;EACD,CAJY;;EAKbC,OAAO,EAAE;IACPC,MAAM,GAAG;MACPR,OAAO,CAACG,IAAD,CAAP,CACGM,IADH,CACQ,MAAM;QACVP,MAAM,CAACQ,IAAP,CAAY,GAAZ;MACD,CAHH,EAIGC,KAJH,CAIUC,KAAD,IAAW;QAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACD,CANH;IAOD;;EATM,CALI;;EAgBbG,OAAO,GAAG;IACRd,kBAAkB,CAACE,IAAD,EAAQa,IAAD,IAAU;MACjC,IAAIA,IAAJ,EAAU;QACR,KAAKV,SAAL,GAAiBU,IAAjB;MACF,CAFA,MAEO;QACL,KAAKV,SAAL,GAAiB,IAAjB;MACF;IACD,CANiB,CAAlB;EAOD;;AAxBY,CAAf","names":["getAuth","signOut","onAuthStateChanged","router","auth","name","data","CheckAuth","methods","logout","then","push","catch","error","console","log","created","user"],"sourceRoot":"","sources":["/home/rani/Desktop/programmation/web dev/myprojectvuejs/src/App.vue"],"sourcesContent":["<template>\n  <div>\n    <nav class=\"navbar navbar-expand-lg bg-light\">\n      <div class=\"container-fluid\">\n        <div class=\"collapse navbar-collapse\" id=\"navbarNav\">\n          <ul class=\"navbar-nav\">\n            <li class=\"nav-item\">\n              <router-link class=\"nav-link active\" aria-current=\"page\" to=\"/\"\n                >Home</router-link\n              >\n            </li>\n            <li class=\"nav-item\">\n              <router-link class=\"nav-link\" v-if=\"CheckAuth\" to=\"/products\"\n                >Products</router-link\n              >\n            </li>\n            <li class=\"nav-item\">\n              <router-link class=\"nav-link\" v-if=\"CheckAuth\" to=\"/myproducts\"\n                >My Products</router-link\n              >\n            </li>\n            <li class=\"nav-item\">\n              <router-link class=\"nav-link\" v-if=\"CheckAuth == null\" to=\"/about\"\n                >About</router-link\n              >\n            </li>\n            <li class=\"nav-item\">\n              <router-link\n                class=\"nav-link\"\n                v-if=\"CheckAuth == null\"\n                to=\"/signin\"\n                >Sign Up</router-link\n              >\n            </li>\n            <li class=\"nav-item\">\n              <router-link class=\"nav-link\" to=\"/login\">Log In</router-link>\n            </li>\n            <li class=\"nav-item\">\n              <a class=\"nav-link\" v-if=\"CheckAuth\" v-on:click=\"logout\"\n                >Logout</a\n              >\n            </li>\n          </ul>\n        </div>\n      </div>\n    </nav>\n  </div>\n\n  <router-view />\n</template>\n\n<script>\nimport \"./Firebase\";\nimport { getAuth, signOut, onAuthStateChanged } from \"firebase/auth\";\nimport router from \"./router\";\nconst auth = getAuth();\nexport default {\n  name: \"App\",\n  data() {\n    return { CheckAuth: null };\n  },\n  methods: {\n    logout() {\n      signOut(auth)\n        .then(() => {\n          router.push(\"/\");\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n  created() {\n    onAuthStateChanged(auth, (user) => {\n      if (user) {\n        this.CheckAuth = user;\n      } else {\n        this.CheckAuth = null;\n      }\n    });\n  },\n};\n</script>\n\n\n\n"]},"metadata":{},"sourceType":"module"}