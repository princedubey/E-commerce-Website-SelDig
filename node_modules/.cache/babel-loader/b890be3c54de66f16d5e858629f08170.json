{"ast":null,"code":"// @ is an alias to /src\nimport \"../Firebase\";\nimport { getAuth, createUserWithEmailAndPassword } from \"firebase/auth\";\nimport { getFirestore, collection, addDoc } from \"firebase/firestore\";\nconst auth = getAuth();\nconst db = getFirestore();\nconst dbRef = collection(db, \"users\");\nimport router from \"../router\";\nexport default {\n  name: \"SignUpView\",\n\n  data() {\n    return {\n      user: {\n        name: \"\",\n        email: \"\",\n        address: \"\",\n        password: \"\",\n        number: \"\",\n        uid: \"\"\n      }\n    };\n  },\n\n  methods: {\n    signupUser() {\n      createUserWithEmailAndPassword(auth, this.user.email, this.user.password).then(dataUser => {\n        addDoc(dbRef, {\n          name: this.user.name,\n          email: this.user.email,\n          address: this.user.address,\n          number: this.user.number,\n          uid: dataUser.user.uid\n        });\n        router.push(\"/products\");\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2DA;AACA,OAAO,aAAP;AACA,SAASA,OAAT,EAAkBC,8BAAlB,QAAwD,eAAxD;AACA,SAASC,YAAT,EAAuBC,UAAvB,EAAmCC,MAAnC,QAAiD,oBAAjD;AACA,MAAMC,IAAG,GAAIL,OAAO,EAApB;AACA,MAAMM,EAAC,GAAIJ,YAAY,EAAvB;AACA,MAAMK,KAAI,GAAIJ,UAAU,CAACG,EAAD,EAAK,OAAL,CAAxB;AACA,OAAOE,MAAP,MAAmB,WAAnB;AACA,eAAe;EACbC,IAAI,EAAE,YADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJF,IAAI,EAAE,EADF;QAEJG,KAAK,EAAE,EAFH;QAGJC,OAAO,EAAE,EAHL;QAIJC,QAAQ,EAAE,EAJN;QAKJC,MAAM,EAAE,EALJ;QAMJC,GAAG,EAAE;MAND;IADD,CAAP;EAUD,CAbY;;EAcbC,OAAO,EAAE;IACPC,UAAU,GAAG;MACXjB,8BAA8B,CAACI,IAAD,EAAO,KAAKM,IAAL,CAAUC,KAAjB,EAAwB,KAAKD,IAAL,CAAUG,QAAlC,CAA9B,CACGK,IADH,CACSC,QAAD,IAAc;QAClBhB,MAAM,CAACG,KAAD,EAAQ;UACZE,IAAI,EAAE,KAAKE,IAAL,CAAUF,IADJ;UAEZG,KAAK,EAAE,KAAKD,IAAL,CAAUC,KAFL;UAGZC,OAAO,EAAE,KAAKF,IAAL,CAAUE,OAHP;UAIZE,MAAM,EAAE,KAAKJ,IAAL,CAAUI,MAJN;UAKZC,GAAG,EAAEI,QAAQ,CAACT,IAAT,CAAcK;QALP,CAAR,CAAN;QAQAR,MAAM,CAACa,IAAP,CAAY,WAAZ;