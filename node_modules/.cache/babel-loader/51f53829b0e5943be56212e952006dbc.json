{"ast":null,"code":"import \"../Firebase\";\nimport { getAuth, signInWithEmailAndPassword } from \"firebase/auth\";\nconst auth = getAuth();\nimport router from \"../router\";\nexport default {\n  name: \"LoginView\",\n\n  data() {\n    return {\n      user: {\n        email: \"\",\n        password: \"\"\n      },\n      messageError: \"\"\n    };\n  },\n\n  methods: {\n    loginUser() {\n      signInWithEmailAndPassword(auth, this.user.email, this.user.password).then(dataUser => {\n        localStorage.setItem(\"uidUser\", dataUser.user.uid);\n        router.push(\"/products\");\n      }).catch(() => {\n        this.messageError = \"invalid email and password\";\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqCA,OAAO,aAAP;AACA,SAASA,OAAT,EAAkBC,0BAAlB,QAAoD,eAApD;AACA,MAAMC,IAAG,GAAIF,OAAO,EAApB;AACA,OAAOG,MAAP,MAAmB,WAAnB;AACA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE,EADH;QAEJC,QAAQ,EAAE;MAFN,CADD;MAKLC,YAAY,EAAE;IALT,CAAP;EAOD,CAVY;;EAWbC,OAAO,EAAE;IACPC,SAAS,GAAG;MACVV,0BAA0B,CAACC,IAAD,EAAO,KAAKI,IAAL,CAAUC,KAAjB,EAAwB,KAAKD,IAAL,CAAUE,QAAlC,CAA1B,CACGI,IADH,CACSC,QAAD,IAAc;QAClBC,YAAY,CAACC,OAAb,CAAqB,SAArB,EAAgCF,QAAQ,CAACP,IAAT,CAAcU,GAA9C;QACAb,MAAM,CAACc,IAAP,CAAY,WAAZ;MACD,CAJH,EAKGC,KALH,CAKS,MAAM;QACX,KAAKT,YAAL,GAAoB,4BAApB;MACD,CAPH;IAQD;;EAVM;AAXI,CAAf","names":["getAuth","signInWithEmailAndPassword","auth","router","name","data","user","email","password","messageError","methods","loginUser","then","dataUser","localStorage","setItem","uid","push","catch"],"sourceRoot":"","sources":["/home/rani/Desktop/programmation/web dev/myprojectvuejs/src/views/LoginView.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <br />\n    <h2>Welcome Back !</h2>\n    <br />\n    <form @submit.prevent=\"loginUser\">\n      <div class=\"mb-3\">\n        <label for=\"exampleInputEmail1\" class=\"form-label\">Email address</label>\n        <input\n          type=\"email\"\n          class=\"form-control\"\n          id=\"exampleInputEmail1\"\n          aria-describedby=\"emailHelp\"\n          v-model=\"user.email\"\n        />\n        <div id=\"emailHelp\" class=\"form-text\">\n          We'll never share your email with anyone else.\n        </div>\n      </div>\n      <div class=\"mb-3\">\n        <label for=\"exampleInputPassword1\" class=\"form-label\">Password</label>\n        <input\n          type=\"password\"\n          class=\"form-control\"\n          id=\"exampleInputPassword1\"\n          v-model=\"user.password\"\n        />\n      </div>\n      <div class=\"alert alert-danger\" v-if=\"messageError\">\n        {{ messageError }}\n      </div>\n      <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport \"../Firebase\";\nimport { getAuth, signInWithEmailAndPassword } from \"firebase/auth\";\nconst auth = getAuth();\nimport router from \"../router\";\nexport default {\n  name: \"LoginView\",\n  data() {\n    return {\n      user: {\n        email: \"\",\n        password: \"\",\n      },\n      messageError: \"\",\n    };\n  },\n  methods: {\n    loginUser() {\n      signInWithEmailAndPassword(auth, this.user.email, this.user.password)\n        .then((dataUser) => {\n          localStorage.setItem(\"uidUser\", dataUser.user.uid);\n          router.push(\"/products\");\n        })\n        .catch(() => {\n          this.messageError = \"invalid email and password\";\n        });\n    },\n  },\n};\n</script>"]},"metadata":{},"sourceType":"module"}