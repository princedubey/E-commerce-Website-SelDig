{"ast":null,"code":"// @ is an alias to /src\nimport \"../Firebase\";\nimport { addDoc, collection, query, getFirestore, onSnapshot, updateDoc, doc, deleteDoc } from \"firebase/firestore\";\nconst db = getFirestore();\nconst q = query(collection(db, \"products\"));\nexport default {\n  name: \"MyProductsView\",\n\n  data() {\n    return {\n      product: {\n        name: \"\",\n        price: 0,\n        image: \"\",\n        description: \"\",\n        uid: localStorage.getItem(\"uidUser\")\n      },\n      dataProduct: {\n        name: '',\n        price: 0,\n        image: '',\n        description: '',\n        key: ''\n      },\n      messageSuccess: \"\",\n      messageUpdateSuccess: \"\",\n      products: []\n    };\n  },\n\n  methods: {\n    AddProduct() {\n      addDoc(q, this.product).then(() => {\n        this.messageSuccess = \"Added\";\n        this.product.name = \"\";\n        this.product.price = \"\";\n        this.product.image = \"\";\n        this.product.description = \"\";\n      });\n    },\n\n    getData(name, price, description, image, key) {\n      this.dataProduct.name = name;\n      this.dataProduct.price = price;\n      this.dataProduct.description = description;\n      this.dataProduct.image = image;\n      this.dataProduct.key = key;\n    },\n\n    getDataKey(key) {\n      this.dataProduct.key = key;\n    },\n\n    UpdateProduct() {\n      const DocRef = doc(db, \"products\", this.dataProduct.key);\n      updateDoc(DocRef, this.dataProduct).then(() => {\n        this.messageUpdateSuccess = \"Updated\";\n        this.dataProduct.name = \"\";\n        this.dataProduct.price = \"\";\n        this.dataProduct.image = \"\";\n        this.dataProduct.description = \"\";\n      });\n    },\n\n    DeleteProduct(key) {\n      deleteDoc(doc(db, \"products\", key));\n    }\n\n  },\n\n  created() {\n    onSnapshot(q, querySnapshot => {\n      this.products = [];\n      querySnapshot.forEach(doc => {\n        if (doc.data().uid == this.product.uid) {\n          this.products.push({\n            key: doc.id,\n            name: doc.data().name,\n            description: doc.data().description,\n            price: doc.data().price,\n            image: doc.data().image\n          });\n        }\n      });\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAkIA;AACA,OAAO,aAAP;AAEA,SACEA,MADF,EAEEC,UAFF,EAGEC,KAHF,EAIEC,YAJF,EAKEC,UALF,EAMEC,SANF,EAOEC,GAPF,EAQEC,SARF,QAUO,oBAVP;AAWA,MAAMC,EAAC,GAAIL,YAAY,EAAvB;AACA,MAAMM,IAAIP,KAAK,CAACD,UAAU,CAACO,EAAD,EAAK,UAAL,CAAX,CAAf;AACA,eAAe;EACbE,IAAI,EAAE,gBADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPF,IAAI,EAAE,EADC;QAEPG,KAAK,EAAE,CAFA;QAGPC,KAAK,EAAE,EAHA;QAIPC,WAAW,EAAE,EAJN;QAKPC,GAAG,EAAEC,YAAY,CAACC,OAAb,CAAqB,SAArB;MALE,CADJ;MAQLC,WAAW,EAAE;QACXT,IAAI,EAAE,EADK;QAEXG,KAAK,EAAE,CAFI;QAGXC,KAAK,EAAE,EAHI;QAIXC,WAAW,EAAE,EAJF;QAKXK,GAAG,EAAE;MALM,CARR;MAeLC,cAAc,EAAE,EAfX;MAgBLC,oBAAoB,EAAE,EAhBjB;MAiBLC,QAAQ,EAAE;IAjBL,CAAP;EAmBD,CAtBY;;EAuBbC,OAAO,EAAE;IACPC,UAAU,GAAG;MACXzB,MAAM,CACJS,CADI,EAGJ,KAAKG,OAHD,CAAN,CAIEc,IAJF,CAIO,MAAM;QACX,KAAKL,cAAL,GAAsB,OAAtB;QACA,KAAKT,OAAL,CAAaF,IAAb,GAAoB,EAApB;QACA,KAAKE,OAAL,CAAaC,KAAb,GAAqB,EAArB;QACA,KAAKD,OAAL,CAAaE,KAAb,GAAqB,EAArB;QACA,KAAKF,OAAL,CAAaG,WAAb,GAA2B,EAA3B;MACD,CAVD;IAWD,CAbM;;IAcPY,OAAO,CAACjB,IAAD,EAAOG,KAAP,EAAcE,WAAd,EAA2BD,KAA3B,EAAkCM,GAAlC,EAAuC;MAC5C,KAAKD,WAAL,CAAiBT,IAAjB,GAAwBA,IAAxB;MACA,KAAKS,WAAL,CAAiBN,KAAjB,GAAyBA,KAAzB;MACA,KAAKM,WAAL,CAAiBJ,WAAjB,GAA+BA,WAA/B;MACA,KAAKI,WAAL,CAAiBL,KAAjB,GAAyBA,KAAzB;MACA,KAAKK,WAAL,CAAiBC,GAAjB,GAAuBA,GAAvB;IACD,CApBM;;IAqBPQ,UAAU,CAACR,GAAD,EAAM;MACd,KAAKD,WAAL,CAAiBC,GAAjB,GAAuBA,GAAvB;IACD,CAvBM;;IAyBPS,aAAa,GAAG;MACd,MAAMC,MAAK,GAAIxB,GAAG,CAACE,EAAD,EAAK,UAAL,EAAiB,KAAKW,WAAL,CAAiBC,GAAlC,CAAlB;MACAf,SAAS,CAACyB,MAAD,EAAS,KAAKX,WAAd,CAAT,CAAoCO,IAApC,CAAyC,MAAM;QAC7C,KAAKJ,oBAAL,GAA4B,SAA5B;QACA,KAAKH,WAAL,CAAiBT,IAAjB,GAAwB,EAAxB;QACA,KAAKS,WAAL,CAAiBN,KAAjB,GAAyB,EAAzB;QACA,KAAKM,WAAL,CAAiBL,KAAjB,GAAyB,EAAzB;QACA,KAAKK,WAAL,CAAiBJ,WAAjB,GAA