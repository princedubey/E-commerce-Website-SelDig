{"ast":null,"code":"import \"../Firebase\";\nimport { getAuth, signOut, onAuthStateChanged } from \"firebase/auth\";\nimport router from \"../router\";\nconst auth = getAuth();\nexport default {\n  name: \"NavbarView\",\n\n  data() {\n    return {\n      CheckAuth: null\n    };\n  },\n\n  methods: {\n    logout() {\n      signOut(auth).then(() => {\n        localStorage.removeItem(\"uidUser\");\n        router.push(\"/\");\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  },\n\n  created() {\n    onAuthStateChanged(auth, user => {\n      if (user) {\n        this.CheckAuth = user;\n      } else {\n        this.CheckAuth = null;\n      }\n    });\n  },\n\n  computed: {\n    count() {\n      return this.$store.state.count;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkHA,OAAO,aAAP;AACA,SAASA,OAAT,EAAkBC,OAAlB,EAA2BC,kBAA3B,QAAqD,eAArD;AACA,OAAOC,MAAP,MAAmB,WAAnB;AACA,MAAMC,IAAG,GAAIJ,OAAO,EAApB;AACA,eAAe;EACbK,IAAI,EAAE,YADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MAAEC,SAAS,EAAE;IAAb,CAAP;EACD,CAJY;;EAKbC,OAAO,EAAE;IACPC,MAAM,GAAG;MACPR,OAAO,CAACG,IAAD,CAAP,CACGM,IADH,CACQ,MAAM;QACVC,YAAY,CAACC,UAAb,CAAwB,SAAxB;QACAT,MAAM,CAACU,IAAP,CAAY,GAAZ;MACD,CAJH,EAKGC,KALH,CAKUC,KAAD,IAAW;QAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACD,CAPH;IAQD;;EAVM,CALI;;EAiBbG,OAAO,GAAG;IACRhB,kBAAkB,CAACE,IAAD,EAAQe,IAAD,IAAU;MACjC,IAAIA,IAAJ,EAAU;QACR,KAAKZ,SAAL,GAAiBY,IAAjB;MACF,CAFA,MAEO;QACL,KAAKZ,SAAL,GAAiB,IAAjB;MACF;IACD,CANiB,CAAlB;EAOD,CAzBY;;EA0Bba,QAAQ,EAAE;IACRC,KAAK,GAAG;MACN,OAAO,KAAKC,MAAL,CAAYC,KAAZ,CAAkBF,KAAzB;IACD;;EAHO;AA1BG,CAAf","names":["getAuth","signOut","onAuthStateChanged","router","auth","name","data","CheckAuth","methods","logout","then","localStorage","removeItem","push","catch","error","console","log","created","user","computed","count","$store","state"],"sourceRoot":"","sources":["/home/rani/Desktop/programmation/web dev/myprojectvuejs/src/views/NavbarView.vue"],"sourcesContent":["<template>\n  <section class=\"ftco-section\">\n    <div class=