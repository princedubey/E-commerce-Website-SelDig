{"ast":null,"code":"import \"../Firebase\";\nimport { getAuth, signInWithEmailAndPassword } from \"firebase/auth\";\nconst auth = getAuth();\nimport router from \"../router\";\nexport default {\n  name: \"LoginView\",\n\n  data() {\n    return {\n      user: {\n        email: \"\",\n        password: \"\"\n      },\n      messageError: \"\"\n    };\n  },\n\n  methods: {\n    loginUser() {\n      signInWithEmailAndPassword(auth, this.user.email, this.user.password).then(dataUser => {\n        localStorage.setItem(\"uidUser\", dataUser.user.uid);\n        router.push(\"/products\");\n      }).catch(() => {\n        this.messageError = \"invalid email and password\";\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgFA,OAAO,aAAP;AACA,SAASA,OAAT,EAAkBC,0BAAlB,QAAoD,eAApD;AACA,MAAMC,IAAG,GAAIF,OAAO,EAApB;AACA,OAAOG,MAAP,MAAmB,WAAnB;AACA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE,EADH;QAEJC,QAAQ,EAAE;MAFN,CADD;MAKLC,YAAY,EAAE;IALT,CAAP;EAOD,CAVY;;EAWbC,OAAO,EAAE;IACPC,SAAS,GAAG;MACVV,0BAA0B,CAACC,IAAD,EAAO,KAAKI,IAAL,CAAUC,KAAjB,EAAwB,KAAKD,IAAL,CAAUE,QAAlC,CAA1B,CACGI,IADH,CACSC,QAAD,IAAc;QAClBC,YAAY,CAACC,OAAb,CAAqB,SAArB,EAAgCF,QAAQ,CAACP,IAAT,CAAcU,GAA9C;QACAb,MAAM,CAACc,IAAP,CAAY,WAAZ;MACD,CAJH,EAKGC,KALH,CAKS,MAAM;QACX,KAAKT,YAAL,GAAoB,4BAApB;MACD,CAPH;IAQD;;EAVM;AAXI,CAAf","names":["getAuth","signInWithEmailAndPassword","auth","router","name","data","user","email","password","messageError","methods","loginUser","then","dataUser","localStorage","setItem","uid","push","catch"],"sourceRoot":"","sources":["/home/rani/Desktop/programmation/web dev/myprojectvuejs/src/views/LoginView.vue"],"sourcesContent":["<template>\n  <section class=\"vh-100\">\n    <div class=\"container-fluid\">\n      <div class=\"row\">\n        <div class=\"col-sm-6 text-black\">\n          <div class=\"px-5 ms-xl-4\">\n            <i\n              class=\"fas fa-crow fa-2x me-3 pt-5 mt-xl-4\"\n              style=\"color: #709085\"\n      