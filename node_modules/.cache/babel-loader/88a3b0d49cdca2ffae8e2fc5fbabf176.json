ubmit.prevent=\"AddProduct\">\n              <input class=\"form-control\" type=\"text\" placeholder=\"name\" aria-label=\"default input example\"\n                v-model=\"product.name\" />\n              <input class=\"form-control\" type=\"number\" placeholder=\"Price\" aria-label=\"default input example\"\n                v-model=\"product.price\" />\n              <input class=\"form-control\" type=\"text\" placeholder=\"image\" aria-label=\"Add image link\"\n                v-model=\"product.image\" />\n\n              <textarea class=\"form-control\" id=\"exampleFormControlTextarea1\" rows=\"3\" placeholder=\"Description\"\n                v-model=\"product.description\"></textarea>\n              <div class=\"alert alert-success\" v-if=\"messageSuccess\">\n                {{ messageSuccess }}\n              </div>\n              <button type=\"submit\" class=\"btn btn-success\">Add</button>\n            </form>\n            <!-- form end here -->\n          </div>\n          <div class=\"modal-footer\">\n            <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">\n              Close\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- Modal 2 (update) -->\n    <div class=\"modal fade\" id=\"update\" data-bs-backdrop=\"static\" data-bs-keyboard=\"false\" tabindex=\"-1\"\n      aria-labelledby=\"staticBackdropLabel\" aria-hidden=\"true\">\n      <div class=\"modal-dialog\">\n        <div class=\"modal-content\">\n          <div class=\"modal-header\">\n            <h5 class=\"modal-title\" id=\"staticBackdropLabel\">Update Product</h5>\n            <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n          </div>\n          <div class=\"modal-body\">\n            .\n            <!-- form start here -->\n            <form @submit.prevent=\"UpdateProduct\">\n              <input class=\"form-control\" type=\"text\" placeholder=\"name\" aria-label=\"default input example\"\n                v-model=\"dataProduct.name\">\n              <textarea class=\"form-control\" id=\"exampleFormControlTextarea1\" rows=\"3\" placeholder=\"Description\"\n                v-model=\"dataProduct.description\"></textarea>\n              <input class=\"form-control\" type=\"text\" placeholder=\"image\" aria-label=\"default input example\"\n                v-model=\"dataProduct.image\">\n              <input class=\"form-control\" type=\"number\" pla ceholder=\"price\" aria-label=\"default input example\"\n                v-model=\"dataProduct.price\">\n              <div class=\"alert alert-success\" v-if=\"messageSuccess\">\n                {{ messageSuccess }}\n              </div>\n              <button type=\"submit\" class=\"btn btn-success\">Add</button>\n            </form>\n            <!-- form end here -->\n          </div>\n          <div class=\"modal-footer\">\n            <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">\n              Close\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport \"../Firebase\";\n\nimport {\n  addDoc,\n  collection,\n  query,\n  getFirestore,\n  onSnapshot,\n  updateDoc,\n  doc\n\n} from \"firebase/firestore\";\nconst db = getFirestore();\nconst q = query(collection(db, \"products\"));\nexport default {\n  name: \"MyProductsView\",\n  data() {\n    return {\n      product: {\n        name: \"\",\n        price: 0,\n        image: \"\",\n        description: \"\",\n        uid: localStorage.getItem(\"uidUser\"),\n      },\n      dataProduct: {\n        name: '',\n        price: 0,\n        image: '',\n        description: '',\n        key: ''\n      },\n      messageSuccess: \"\",\n      products: [],\n    };\n  },\n  methods: {\n    AddProduct() {\n      addDoc(\n        q,\n\n        this.product\n      ).then(() => {\n        this.messageSuccess = \"Added\";\n        this.product.name = \"\";\n        this.product.price = \"\";\n        this.product.image = \"\";\n        this.product.description = \"\";\n      });\n    },\n    getData(name, price, description, image, key) {\n      this.dataProduct.name = name\n      this.dataProduct.price = price\n      this.dataProduct.description = description\n      this.dataProduct.image = image\n      this.dataProduct.key = key\n    },\n    UpdateProduct() {\n      const DocRef = doc(db, \"products\", this.dataProduct.key);\n      updateDoc(DocRef, this.dataProduct);\n\n    }\n  },\n  created() {\n\n    onSnapshot(q, (querySnapshot) => {\n      this.products = [];\n      querySnapshot.forEach((doc) => {\n        if (doc.data().uid == this.product.uid) {\n          this.products.push({\n            key: doc.id,\n            name: doc.data().name,\n            description: doc.data().description,\n            price: doc.data().price,\n            image: doc.data().image,\n          });\n\n        }\n\n      });\n    });\n  }\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}