{"ast":null,"code":"// @ is an alias to /src\nimport \"../Firebase\";\nimport { addDoc, collection, query, getFirestore, onSnapshot, updateDoc, doc } from \"firebase/firestore\";\nconst db = getFirestore();\nconst q = query(collection(db, \"products\"));\nexport default {\n  name: \"MyProductsView\",\n\n  data() {\n    return {\n      product: {\n        name: \"\",\n        price: 0,\n        image: \"\",\n        description: \"\",\n        uid: localStorage.getItem(\"uidUser\")\n      },\n      dataProduct: {\n        name: '',\n        price: 0,\n        image: '',\n        description: '',\n        key: ''\n      },\n      messageSuccess: \"\",\n      products: []\n    };\n  },\n\n  methods: {\n    AddProduct() {\n      addDoc(q, this.product).then(() => {\n        this.messageSuccess = \"Added\";\n        this.product.name = \"\";\n        this.product.price = \"\";\n        this.product.image = \"\";\n        this.product.description = \"\";\n      });\n    },\n\n    getData(name, price, description, image, key) {\n      this.dataProduct.name = name;\n      this.dataProduct.price = price;\n      this.dataProduct.description = description;\n      this.dataProduct.image = image;\n      this.dataProduct.key = key;\n    },\n\n    UpdateProduct() {\n      const DocRef = doc(db, \"products\", this.dataProduct.key);\n      updateDoc(DocRef, this.dataProduct);\n    }\n\n  },\n\n  created() {\n    onSnapshot(q, querySnapshot => {\n      this.products = [];\n      querySnapshot.forEach(doc => {\n        if (doc.data().uid == this.product.uid) {\n          this.products.push({\n            key: doc.id,\n            name: doc.data().name,\n            description: doc.data().description,\n            price: doc.data().price,\n            image: doc.data().image\n          });\n        }\n      });\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAsGA;AACA,OAAO,aAAP;AAEA,SACEA,MADF,EAEEC,UAFF,EAGEC,KAHF,EAIEC,YAJF,EAKEC,UALF,EAMEC,SANF,EAOEC,GAPF,QASO,oBATP;AAUA,MAAMC