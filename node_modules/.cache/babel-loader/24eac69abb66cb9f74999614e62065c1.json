{"ast":null,"code":"import \"../Firebase\";\nimport { getAuth, signOut, onAuthStateChanged } from \"firebase/auth\";\nimport router from \"../router\";\nconst auth = getAuth();\nexport default {\n  name: \"NavbarView\",\n\n  data() {\n    return {\n      CheckAuth: null\n    };\n  },\n\n  methods: {\n    logout() {\n      signOut(auth).then(() => {\n        localStorage.removeItem(\"uidUser\");\n        router.push(\"/\");\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  },\n\n  created() {\n    onAuthStateChanged(auth, user => {\n      if (user) {\n        this.CheckAuth = user;\n      } else {\n        this.CheckAuth = null;\n      }\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAgKA,OAAO,aAAP;AACA,SAASA,OAAT,EAAkBC,OAAlB,EAA2BC,kBAA3B,QAAqD,eAArD;AACA,OAAOC,MAAP,MAAmB,WAAnB;AACA,MAAMC,IAAG,GAAIJ,OAAO,EAApB;AACA,eAAe;EACbK,IAAI,EAAE,YADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MAAEC,SAAS,EAAE;IAAb,CAAP;EACD,CAJY;;EAKbC,OAAO,EAAE;IACPC,MAAM,GAAG;MACPR,OAAO,CAACG,IAAD,CAAP,CACGM,IADH,CACQ,MAAM;QACVC,YAAY,CAACC,UAAb,CAAwB,SAAxB;QACAT,MAAM,CAACU,IAAP,CAAY,GAAZ;MACD,CAJH,EAKGC,KALH,CAKUC,KAAD,IAAW;QAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACD,CAPH;IAQD;;EAVM,CALI;;EAiBbG,OAAO,GAAG;IACRhB,kBAAkB,CAACE,IAAD,EAAQe,IAAD,IAAU;MACjC,IAAIA,IAAJ,EAAU;QACR,KAAKZ,SAAL,GAAiBY,IAAjB;MACF,CAFA,MAEO;QACL,KAAKZ,SAAL,GAAiB,IAAjB;MACF;IACD,CANiB,CAAlB;EAOD;;AAzBY,CAAf","names":["getAuth","signOut","onAuthStateChanged","router","auth","name","data","CheckAuth","methods","logout","then","localStorage","removeItem","push","catch","error","console","log","created","user"],"sourceRoot":"","sources":["/home/rani/Desktop/programmation/web dev/myprojectvuejs/src/views/NavbarView.vue"],"sourcesContent":["<template>\n  <nav class=\"navbar navbar-expand-lg bg-light\">\n    <div class=\"container\">\n      <button\n        class=\"navbar-toggler\"\n        type=\"button\"\n        data-bs-toggle=\"collapse\"\n        data-bs-target=\"#navbarNav\"\n        aria-controls=\"navbarTog