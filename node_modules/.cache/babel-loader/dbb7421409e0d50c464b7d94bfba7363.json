{"ast":null,"code":"import { collection, query, getFirestore, onSnapshot } from \"firebase/firestore\";\nconst db = getFirestore();\nconst q = query(collection(db, \"products\"));\nexport default {\n  name: \"ProductsView\",\n\n  data() {\n    return {\n      products: []\n    };\n  },\n\n  created() {\n    onSnapshot(q, querySnapshot => {\n      this.products = [];\n      querySnapshot.forEach(doc => {\n        console.log(doc.data());\n        this.products.push({\n          key: doc.id,\n          name: doc.data().name,\n          description: doc.data().description,\n          price: doc.data().price,\n          image: doc.data().image\n        });\n      });\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAoBA,SACEA,UADF,EAEEC,KAFF,EAGEC,YAHF,EAIEC,UAJF,QAKO,oBALP;AAMA,MAAMC,EAAC,GAAIF,YAAY,EAAvB;AACA,MAAMG,IAAIJ,KAAK,CAACD,UAAU,CAACI,EAAD,EAAK,UAAL,CAAX,CAAf;AACA,eAAe;EACbE,IAAI,EAAE,cADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IADL,CAAP;EAGD,CANY;;EAObC,OAAO,GAAG;IAERN,UAAU,CAACE,CAAD,EAAKK,aAAD,IAAmB;MAC/B,KAAKF,QAAL,GAAgB,EAAhB;MACAE,aAAa,CAACC,OAAd,CAAuBC,GAAD,IAAS;QAC7BC,OAAO,CAACC,GAAR,CAAYF,GAAG,CAACL,IAAJ,EAAZ;QACA,KAAKC,QAAL,CAAcO,IAAd,CAAmB;UACjBC,GAAG,EAAEJ,GAAG,CAACK,EADQ;UAEjBX,IAAI,EAAEM,GAAG,CAACL,IAAJ,GAAWD,IAFA;UAGjBY,WAAW,EAAEN,GAAG,CAACL,IAAJ,GAAWW,WAHP;UAIjBC,KAAK,EAAEP,GAAG,CAACL,IAAJ,GAAWY,KAJD;UAKjBC,KAAK,EAAER,GAAG,CAACL,IAAJ,GAAWa;QALD,CAAnB;MAOD,CATD;IAUD,CAZS,CAAV;EAaD;;AAtBY,CAAf","names":["collection","query","getFirestore","onSnapshot","db","q","name","data","products","created","querySnapshot","forEach","doc","console","log","push","key","id","description","price","image"],"sourceRoot":"","sources":["/home/rani/Desktop/programmation/web dev/myprojectvuejs/src/views/ProductsView.vue"],"sourcesContent":["<template>\n  <br />\n  <div class=\"container\">\n    <h1 style=\"text-align: center\">Products</h1>\n    <br />\n    <div class=\"row\">\n      <div class=\"card\" style=\"width: 18rem\" v-for=\"item in products\" :key=\"item.name\">\n        <img :src=\"item.image\" class=\"card-img-top\" alt=\"../assets/logo.png\" />\n        <div class=\"card-body\">\n          <h5 class=\"card-title\">{{ item.name }}</h5>\n          <h5 class=\"card-title\">{{ item.price }}</h5>\n          <p class=\"card-text\">{{ item.description }}</p>\n          <a href=\"#\" class=\"btn btn-primary\">Go somewhere</a>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {\n  collection,\n  query,\n  getFirestore,\n  onSnapshot,\n} from \"firebase/firestore\";\nconst db = getFirestore();\nconst q = query(collection(db, \"products\"));\nexport default {\n  name: \"ProductsView\",\n  data() {\n    return {\n      products: [],\n    };\n  },\n  created() {\n\n    onSnapshot(q, (querySnapshot) => {\n      this.products = [];\n      querySnapshot.forEach((doc) => {\n        console.log(doc.data());\n        this.products.push({\n          key: doc.id,\n          name: doc.data().name,\n          description: doc.data().description,\n          price: doc.data().price,\n          image: doc.data().image,\n        });\n      });\n    });\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}