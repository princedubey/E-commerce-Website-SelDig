{"ast":null,"code":"// @ is an alias to /src\nimport \"../Firebase\";\nimport { addDoc, collection, query, getFirestore, onSnapshot, updateDoc, doc, deleteDoc } from \"firebase/firestore\";\nimport { getStorage, ref } from \"firebase/storage\"; // Create a root reference\n\nconst storage = getStorage();\nconst db = getFirestore();\nconst q = query(collection(db, \"products\"));\nexport default {\n  name: \"MyProductsView\",\n\n  data() {\n    return {\n      product: {\n        name: \"\",\n        price: 0,\n        image: \"\",\n        description: \"\",\n        uid: localStorage.getItem(\"uidUser\")\n      },\n      dataProduct: {\n        name: '',\n        price: 0,\n        image: '',\n        description: '',\n        key: ''\n      },\n      messageSuccess: \"\",\n      messageUpdateSuccess: \"\",\n      products: [],\n      uploadValue: 0,\n      picture: null,\n      imageData: null,\n      imageName: ''\n    };\n  },\n\n  methods: {\n    AddProduct() {\n      //  upload image start here\n      this.picture = null; //   firebase.storage().ref(`images/${this.imageName}`).put(this.imageData);\n      // Create a reference to 'images/mountains.jpg'\n\n      const storageRef = ref(storage, 'images/${this.imageName}').put(this.imageData);\n      storageRef.on(`state_changed`, snapshot => {\n        this.uploadValue = snapshot.bytesTransferred / snapshot.totalBytes * 100;\n      }, error => {\n        console.log(error.message);\n      }, () => {\n        this.uploadValue = 100;\n        storageRef.snapshot.ref.getDownloadURL().then(url => {\n          console.log(url);\n          this.product.image = url;\n        });\n      });\n      addDoc(q, this.product).then(() => {\n        this.messageSuccess = \"Added\";\n        this.product.name = \"\";\n        this.product.price = \"\";\n        this.product.image = \"\";\n        this.product.description = \"\";\n      });\n    },\n\n    getData(name, price, description, image, key) {\n      this.dataProduct.name = name;\n      this.dataProduct.price = price;\n      this.dataProduct.description = description;\n      this.dataProduct.image = image;\n      this.dataProduct.key = key;\n    },\n\n    getDataKey(key, name) {\n      this.dataProduct.key = key;\n      this.dataProduct.name = name;\n    },\n\n    UpdateProduct() {\n      const DocRef = doc(db, \"products\", this.dataProduct.key);\n      updateDoc(DocRef, this.dataProduct).then(() => {\n        this.messageUpdateSuccess = \"Updated\";\n        this.dataProduct.name = \"\";\n        this.dataProduct.price = \"\";\n        this.dataProduct.image = \"\";\n        this.dataProduct.description = \"\";\n      });\n    },\n\n    DeleteProduct(key) {\n      deleteDoc(doc(db, \"products\", key)).then(() => {\n        document.getElementById('close').click();\n      });\n    },\n\n    previewImage(event) {\n      this.uploadValue = 0;\n      this.picture = null;\n      this.imageData = event.target.files[0];\n      this.imageName = event.target.files[0].name;\n    }\n\n  },\n\n  created() {\n    onSnapshot(q, querySnapshot => {\n      this.products = [];\n      querySnapshot.forEach(doc => {\n        if (doc.data().uid == this.product.uid) {\n          this.products.push({\n            key: doc.id,\n            name: doc.data().name,\n            description: doc.data().description,\n            price: doc.data().price,\n            image: doc.data().image\n          });\n        }\n      });\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAmIA;AACA,OAAO,aAAP;AAEA,SACEA,MADF,EAEEC,UAFF,EAGEC,KAHF,EAIEC,YAJF,EAKEC,UALF,EAMEC,SANF,EAOEC,GAPF,EAQEC,SARF,QAUO,oBAVP;AAWA,SAASC,UAAT,EAAqBC,GAArB,QAAgC,kBAAhC,C,CACA;;AACA,MAAMC,OAAM,GAAIF,UAAU,EAA1B;AAEA,MAAMG,EAAC,GAAIR,YAAY,EAAvB;AACA,MAAMS,IAAIV,KAAK,CAACD,UAAU,CAACU,EAAD,EAAK,UAAL,CAAX,CAAf;AACA,eAAe;EACbE,IAAI,EAAE,gBADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPF,IAAI,EAAE,EADC;QAEPG,KAAK,EAAE,CAFA;QAGPC,KAAK,EAAE,EAHA;QAIPC,WAAW,EAAE,EAJN;QAKPC,GAAG,EAAEC,YAAY,CAACC