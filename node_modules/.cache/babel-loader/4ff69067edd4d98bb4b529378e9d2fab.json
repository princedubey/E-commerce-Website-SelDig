GAA+B,EAA/B;MACD,CAND;IAQD,CArDM;;IAsDPgC,aAAa,CAAC3B,GAAD,EAAM;MACjBhB,SAAS,CAACD,GAAG,CAACK,EAAD,EAAK,UAAL,EAAiBY,GAAjB,CAAJ,CAAT,CAAoCqB,IAApC,CAAyC,MAAM;QAC7CO,QAAQ,CAACC,cAAT,CAAwB,OAAxB,EAAiCC,KAAjC;MACD,CAFD;IAGD,CA1DM;;IA2DPC,YAAY,CAACC,KAAD,EAAQ;MAClB,KAAK5B,WAAL,GAAmB,CAAnB;MACA,KAAKC,OAAL,GAAe,IAAf;MACA,KAAKC,SAAL,GAAiB0B,KAAK,CAACC,MAAN,CAAaC,KAAb,CAAmB,CAAnB,CAAjB;MACA,KAAK3B,SAAL,GAAiByB,KAAK,CAACC,MAAN,CAAaC,KAAb,CAAmB,CAAnB,EAAsB5C,IAAvC;IACF;;EAhEO,CA3BI;;EA8Fb6C,OAAO,GAAG;IAERtD,UAAU,CAACQ,CAAD,EAAK+C,aAAD,IAAmB;MAC/B,KAAKjC,QAAL,GAAgB,EAAhB;MACAiC,aAAa,CAACC,OAAd,CAAuBtD,GAAD,IAAS;QAC7B,IAAIA,GAAG,CAACQ,IAAJ,GAAWK,GAAX,IAAkB,KAAKJ,OAAL,CAAaI,GAAnC,EAAwC;UACtC,KAAKO,QAAL,CAAcmC,IAAd,CAAmB;YACjBtC,GAAG,EAAEjB,GAAG,CAACwD,EADQ;YAEjBjD,IAAI,EAAEP,GAAG,CAACQ,IAAJ,GAAWD,IAFA;YAGjBK,WAAW,EAAEZ,GAAG,CAACQ,IAAJ,GAAWI,WAHP;YAIjBF,KAAK,EAAEV,GAAG,CAACQ,IAAJ,GAAWE,KAJD;YAKjBC,KAAK,EAAEX,GAAG,CAACQ,IAAJ,GAAWG;UALD,CAAnB;QAQF;MAED,CAZD;IAaD,CAfS,CAAV;EAgBF;;AAhHa,CAAf","names":["addDoc","collection","query","getFirestore","onSnapshot","updateDoc","doc","deleteDoc","getStorage","ref","storage","db","q","name","data","product","price","image","description","uid","localStorage","getItem","dataProduct","key","messageSuccess","messageUpdateSuccess","products","uploadValue","picture","imageData","imageName","methods","AddProduct","storageRef","put","on","snapshot","bytesTransferred","totalBytes","error","console","log","message","getDownloadURL","then","url","getData","getDataKey","UpdateProduct","DocRef","DeleteProduct","document","getElementById","click","previewImage","event","target","files","created","querySnapshot","forEach","push","id"],"sourceRoot":"","sources":["/home/rani/Desktop/programmation/web dev/myprojectvuejs/src/views/MyProductsView.vue"],"sourcesContent":["<template>\n  <br />\n  <div class=\"container\">\n    <button type=\"button\" class=\"btn btn-primary\" data-bs-toggle=\"modal\" data-bs-target=\"#exampleModal\">\n      Add new product\n    </button>\n\n    <br />\n    <h1 style=\"text-align: center\">My Products</h1>\n    <br />\n    <div class=\"row\">\n      <div class=\"card\" style=\"width: 18rem\" v-for=\"item in products\" :key=\"item.name\">\n        <img :src=\"item.image\" class=\"card-img-top\" alt=\"...\" />\n        <div class=\"card-body\">\n          <h5 class=\"card-title\">{{ item.name }}</h5>\n          <h5 class=\"card-title\">{{ item.price }}</h5>\n          <p class=\"card-text\">{{ item.description }}</p>\n          <button type=\"button\" class=\"btn btn-primary\">Buy</button>\n          <button type=\"button\" class=\"btn btn-info\" data-bs-toggle=\"modal\" data-bs-target=\"#update\"\n            v-on:click=\"getData(item.name, item.price, item.description, item.image, item.key)\">Update</button>\n          <button type=\"button\" class=\"btn btn-danger\" data-bs-toggle=\"modal\" data-bs-target=\"#delete\"\n            v-on:click=\"getDataKey(item.key, item.name)\">Delete</button>\n\n        </div>\n      </div>\n    </div>\n    <!-- Modal 1 -->\n    <div class=\"modal fade\" id=\"exampleModal\" data-bs-backdrop=\"static\" data-bs-keyboard=\"false\" tabindex=\"-1\"\n      aria-labelledby=\"staticBackdropLabel\" aria-hidden=\"true\">\n      <div class=\"modal-dialog\">\n        <div class=\"modal-content\">\n          <div class=\"modal-header\">\n            <h5 class=\"modal-title\" id=\"staticBackdropLabel\">Add Product</h5>\n            <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n          </div>\n          <div class=\"modal-body\">\n            .\n            <!-- form start here -->\n            <form @submit.prevent=\"AddProduct\">\n              <input class=\"form-control\" type=\"text\" placeholder=\"name\" aria-label=\"default input example\"\n                v-model=\"product.name\" />\n              <input class=\"form-control\" type=\"number\" placeholder=\"Price\" aria-label=\"default input example\"\n                v-model=\"product.price\" />\n              <input class=\"form-control\" type=\"file\" placeholder=\"image\" aria-label=\"Add image link\" accept=\".jpg,.png\"\n                @change=\"previewImage\" />\n\n              <textarea class=\" form-control\" id=\"exampleFormControlTextarea1\" rows=\"3\" placeholder=\"Description\"\n                v-model=\"product.description\"></textarea>\n              <div class=\"alert alert-success\" v-if=\"messageSuccess\">\n                {{ messageSuccess }}\n              </div>\n              <button type=\"submit\" class=\"btn btn-success\">Add</button>\n            </form>\n            <!-- form end here -->\n          </div>\n          <div class=\"modal-footer\">\n            <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">\n              Close\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- Modal 2 (update) -->\n    <div class=\"modal fade\" id=\"update\" data-bs-backdrop=\"static\" data-bs-keyboard=\"false\" tabindex=\"-1\"\n      aria-labelledby=\"staticBackdropLabel\" aria-hidden=\"true\">\n      <div class=\"modal-dialog\">\n        <div class=\"modal-content\">\n          <div class=\"modal-header\">\n            <h5 class=\"modal-title\" id=\"staticBackdropLabel\">Update Product</h5>\n            <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n          </div>\n          <div class=\"modal-body\">\n            .\n            <!-- form start here -->\n            <form @submit.prevent=\"UpdateProduct\">\n              <input class=\"form-control\" type=\"text\" placeholder=\"name\" aria-label=\"default input example\"\n                v-model=\"dataProduct.name\">\n              <textarea class=\"form-control\" id=\"exampleFormControlTextarea1\" rows=\"3\" placeholder=\"Description\"\n                v-model=\"dataProduct.description\"></textarea>\n              <input class=\"form-control\" type=\"text\" placeholder=\"image\" aria-label=\"default input example\"\n                v-model=\"dataProduct.image\">\n              <input class=\"form-control\" type=\"number\" pla ceholder=\"price\" aria-label=\"default input example\"\n                v-model=\"dataProduct.price\">\n              <div class=\"alert alert-success\" v-if=\"messageUpdateSuccess\">\n                {{ messageUpdateSuccess }}\n              </div>\n              <button type=\"submit\" class=\"btn btn-success\">Update</button>\n            </form>\n            <!-- form end here -->\n          </div>\n          <div class=\"modal-footer\">\n            <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">\n              Close\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- Modal 3 (delete) -->\n    <div v-if=\"ShowModel == true\" class=\"modal fade\" id=\"delete\" data-bs-backdrop=\"static\" data-bs-keyboard=\"false\"\n      tabindex=\"-1\" aria-labelledby=\"staticBackdropLabel\" aria-hidden=\"true\">\n      <div class=\"modal-dialog\">\n        <div class=\"modal-content\">\n          <div class=\"modal-header\">\n            <h5 class=\"modal-title\" id=\"staticBackdropLabel\">Delete Product</h5>\n            <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n          </div>\n          <div class=\"modal-body\">\n            .\n            <!-- form start here -->\n            <form>\n              <h3> Do you want to delete this course ?</h3>\n              <br />\n              <h5> Title: {{ dataProduct.name }} </h5>\n            </form>\n            <!-- form end here -->\n          </div>\n          <div class=\"modal-footer\">\n            <button type=\"button\" id=\"close\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">\n              Close\n            </button>\n            <button type=\"button\" class=\"btn btn-danger\" v-on:click=\"DeleteProduct(dataProduct.key)\">Delete</button>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport \"../Firebase\";\n\nimport {\n  addDoc,\n  collection,\n  query,\n  getFirestore,\n  onSnapshot,\n  updateDoc,\n  doc,\n  deleteDoc,\n\n} from \"firebase/firestore\";\nimport { getStorage, ref } from \"firebase/storage\";\n// Create a root reference\nconst storage = getStorage();\n\nconst db = getFirestore();\nconst q = query(collection(db, \"products\"));\nexport default {\n  name: \"MyProductsView\",\n  data() {\n    return {\n      product: {\n        name: \"\",\n        price: 0,\n        image: \"\",\n        description: \"\",\n        uid: localStorage.getItem(\"uidUser\"),\n      },\n      dataProduct: {\n        name: '',\n        price: 0,\n        image: '',\n        description: '',\n        key: ''\n      },\n      messageSuccess: \"\",\n      messageUpdateSuccess: \"\",\n      products: [],\n      uploadValue: 0,\n      picture: null,\n      imageData: null,\n      imageName: '',\n    };\n  },\n  methods: {\n    AddProduct() {\n\n      //  upload image start here\n      this.picture = null;\n      //   firebase.storage().ref(`images/${this.imageName}`).put(this.imageData);\n      // Create a reference to 'images/mountains.jpg'\n      const storageRef = ref(storage, 'images/${this.imageName}').put(this.imageData);\n      storageRef.on(`state_changed`, snapshot => {\n        this.uploadValue = (snapshot.bytesTransferred / snapshot.totalBytes) * 100;\n      }, error => { console.log(error.message) },\n        () => {\n          this.uploadValue = 100;\n          storageRef.snapshot.ref.getDownloadURL().then((url) => {\n            console.log(url)\n            this.product.image = url\n          })\n        })\n\n      addDoc(\n        q,\n\n        this.product\n      ).then(() => {\n        this.messageSuccess = \"Added\";\n        this.product.name = \"\";\n        this.product.price = \"\";\n        this.product.image = \"\";\n        this.product.description = \"\";\n      });\n    },\n    getData(name, price, description, image, key) {\n      this.dataProduct.name = name\n      this.dataProduct.price = price\n      this.dataProduct.description = description\n      this.dataProduct.image = image\n      this.dataProduct.key = key\n    },\n    getDataKey(key, name) {\n      this.dataProduct.key = key\n      this.dataProduct.name = name;\n    },\n\n    UpdateProduct() {\n      const DocRef = doc(db, \"products\", this.dataProduct.key);\n      updateDoc(DocRef, this.dataProduct).then(() => {\n        this.messageUpdateSuccess = \"Updated\";\n        this.dataProduct.name = \"\";\n        this.dataProduct.price = \"\";\n        this.dataProduct.image = \"\";\n        this.dataProduct.description = \"\";\n      });\n\n    },\n    DeleteProduct(key) {\n      deleteDoc(doc(db, \"products\", key)).then(() => {\n        document.getElementById('close').click();\n      })\n    },\n    previewImage(event) {\n      this.uploadValue = 0;\n      this.picture = null;\n      this.imageData = event.target.files[0];\n      this.imageName = event.target.files[0].name\n    }\n  },\n\n  created() {\n\n    onSnapshot(q, (querySnapshot) => {\n      this.products = [];\n      querySnapshot.forEach((doc) => {\n        if (doc.data().uid == this.product.uid) {\n          this.products.push({\n            key: doc.id,\n            name: doc.data().name,\n            description: doc.data().description,\n            price: doc.data().price,\n            image: doc.data().image,\n          });\n\n        }\n\n      });\n    });\n  }\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}