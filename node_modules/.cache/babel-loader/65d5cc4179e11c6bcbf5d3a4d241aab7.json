{"ast":null,"code":"import \"../Firebase\";\nimport { getAuth, signInWithEmailAndPassword } from \"firebase/auth\";\nconst auth = getAuth();\nimport router from \"../router\";\nexport default {\n  name: \"LoginView\",\n\n  data() {\n    return {\n      user: {\n        email: \"\",\n        password: \"\"\n      },\n      messageError: \"\"\n    };\n  },\n\n  methods: {\n    loginUser() {\n      signInWithEmailAndPassword(auth, this.user.email, this.user.password).then(dataUser => {\n        localStorage.setItem(\"uidUser\", dataUser.user.uid);\n        router.push(\"/products\");\n      }).catch(() => {\n        this.messageError = \"invalid email and password\";\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqEA,OAAO,aAAP;AACA,SAASA,OAAT,EAAkBC,0BAAlB,QAAoD,eAApD;AACA,MAAMC,IAAG,GAAIF,OAAO,EAApB;AACA,OAAOG,MAAP,MAAmB,WAAnB;AACA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE,EADH;QAEJC,QAAQ,EAAE;MAFN,CADD;MAKLC,YAAY,EAAE;IALT,CAAP;EAOD,CAVY;;EAWbC,OAAO,EAAE;IACPC,SAAS,GAAG;MACVV,0BAA0B,CAACC,IAAD,EAAO,KAAKI,IAAL,CAAUC,KAAjB,EAAwB,KAAKD,IAAL,CAAUE,QAAlC,CAA1B,CACGI,IADH,CACSC,QAAD,IAAc;QAClBC,YAAY,CAACC,OAAb,CAAqB,SAArB,EAAgCF,QAAQ,CAACP,IAAT,CAAcU,GAA9C;QACAb,MAAM,CAACc,IAAP,CAAY,WAAZ;MACD,CAJH,EAKGC,KALH,CAKS,MAAM;QACX,KAAKT,YAAL,GAAoB,4BAApB;MACD,CAPH;IAQD;;EAVM;AAXI,CAAf","names":["getAuth","signInWithEmailAndPassword","auth","router","name","data","user","email","password","messageError","methods","loginUser","then","dataUser","localStorage","setItem","uid","push","catch"],"sourceRoot":"","sources":["/home/rani/Desktop/programmation/web dev/myprojectvuejs/src/views/LoginView.vue"],"sourcesContent":["<template>\n  <section class=\"vh-100\" style=\"background-color: #508bfc\">\n    <div class=\"container py-5 h-100\">\n      <div class=\"row d-flex justify-content-center align-items-center h-100\">\n        <div class=\"col-12 col-md-8 col-lg-6 col-xl-5\">\n          <div class=\"card shadow-2-strong\" style=\"border-radius: 1rem\">\n            <div class=\"card-body p-5 text-center\">\n              <h3 class=\"mb-5\">Sign in</h3>\n\n              <div class=\"form-outline mb-4\">\n                <input\n                  type=\"email\"\n                  id=\"typeEmailX-2\"\n                  class=\"form-control form-control-lg\"\n                />\n                <label class=\"form-label\" for=\"typeEmailX-2\">Email</label>\n              </div>\n\n              <div class=\"form-outline mb-4\">\n                <input\n                  type=\"password\"\n                  id=\"typePasswordX-2\"\n                  class=\"form-control form-control-lg\"\n                />\n                <label class=\"form-label\" for=\"typePasswordX-2\">Password</label>\n              </div>\n\n              <!-- Checkbox -->\n              <div class=\"form-check d-flex justify-content-start mb-4\">\n                <input\n                  class=\"form-check-input\"\n                  type=\"checkbox\"\n                  value=\"\"\n                  id=\"form1Example3\"\n                />\n                <label class=\"form-check-label\" for=\"form1Example3\">\n                  Remember password\n                </label>\n              </div>\n\n              <button class=\"btn btn-primary btn-lg btn-block\" type=\"submit\">\n                Login\n              </button>\n\n              <hr class=\"my-4\" />\n\n              <button\n                class=\"btn btn-lg btn-block btn-primary\"\n                style=\"background-color: #dd4b39\"\n                type=\"submit\"\n              >\n                <i class=\"fab fa-google me-2\"></i> Sign in with google\n              </button>\n              <button\n                class=\"btn btn-lg btn-block btn-primary mb-2\"\n                style=\"background-color: #3b5998\"\n                type=\"submit\"\n              >\n                <i class=\"fab fa-facebook-f me-2\"></i>Sign in with facebook\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport \"../Firebase\";\nimport { getAuth, signInWithEmailAndPassword } from \"firebase/auth\";\nconst auth = getAuth();\nimport router from \"../router\";\nexport default {\n  name: \"LoginView\",\n  data() {\n    return {\n      user: {\n        email: \"\",\n        password: \"\",\n      },\n      messageError: \"\",\n    };\n  },\n  methods: {\n    loginUser() {\n      signInWithEmailAndPassword(auth, this.user.email, this.user.password)\n        .then((dataUser) => {\n          localStorage.setItem(\"uidUser\", dataUser.user.uid);\n          router.push(\"/products\");\n        })\n        .catch(() => {\n          this.messageError = \"invalid email and password\";\n        });\n    },\n  },\n};\n</script>\n\n<style>\n.gradient-custom {\n  /* fallback for old browsers */\n  background: #6a11cb;\n\n  /* Chrome 10-25, Safari 5.1-6 */\n  background: -webkit-linear-gradient(\n    to right,\n    rgba(106, 17, 203, 1),\n    rgba(37, 117, 252, 1)\n  );\n\n  /* W3C, IE 10+/ Edge, Firefox 16+, Chrome 26+, Opera 12+, Safari 7+ */\n  background: linear-gradient(\n    to right,\n    rgba(106, 17, 203, 1),\n    rgba(37, 117, 252, 1)\n  );\n}\n</style>"]},"metadata":{},"sourceType":"module"}